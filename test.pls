-- Declaración del bloque PL/SQL
DECLARE
  v_nombre VARCHAR2(50);
BEGIN
  -- Asignar un valor a la variable
  v_nombre := 'Juan Perez';
  
  -- Imprimir el valor de la variable
  DBMS_OUTPUT.PUT_LINE('Nombre: ' || v_nombre);
END;
/
